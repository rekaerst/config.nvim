configuration: lua
keymaps, autocmds: vimL, lua

plugins
	do something quickly: python
	runtime efficent: lua
	i am speed: go, rust, c
	portable: vimL

port vimL to lua or something

set dictionary to /usr/share/dict/words to enable word complete

structure:
	init.lua		-- vim options and keymaps
		utils 		-- common commands
		plugins		-- packer 
		config		-- plugin configuration
