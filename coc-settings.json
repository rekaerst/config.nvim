{
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": [
				"start"
			],
			"filetypes": [
				"zsh",
				"sh",
				"make"
			],
			"ignoredRootPaths": [
				"/"
			]
		},
		"sql": {
			"command": "sqls",
			"args": [
				"-config",
				"$HOME/.config/sqls/config.yml"
			],
			"filetypes": [
				"sql"
			],
			"shell": true
		}
	},
	"coc.source.file.ignoreHidden": false,
	"clangd.fallbackFlags": [
		// "-I/usr/lib/gcc/x86_64-linux-gnu/10/include"
	],
	"clangd.arguments": [
		"--clang-tidy",
		"--fallback-style",
		"Google"
	],
	"java.home": "/usr/lib/jvm/java-17-openjdk-amd64",
	"java.completion.filteredTypes": [
		"java.awt.List",
		"com.sun.*"
	],
	"java.signatureHelp.enabled": true,
	"java.jdt.ls.home": "/home/arthur/.local/share/jdtls",
	"python.pythonPath": "/usr/bin/python3",
	"python.linting.pylintArgs": [
		"--extension-pkg-whitelist=PyQt5"
	],
	"python.autoComplete.addBrackets": true,
	"diagnostic.errorSign": "â˜¢",
	"suggest.completionItemKindLabels": {
		"variable": "\uf6a5",
		"constant": "\uf6a5",
		"function": "\uf6a6",
		"method": "\uf6a6",
		"module": "\uf673",
		"struct": "\uf0e8",
		"class": "\ufb44",
		"interface": "\uf805",
		"keyword": "\uf767",
		"text": "\uf719",
		"enum": "\uf636",
		"enumMember": "\uf02b",
		"color": "\ue22b",
		"property": "\uf48d",
		"field": "\uf01c",
		"unit": "\uf767",
		"file": "\uf016",
		"value": "\uf471",
		"event": "\ufacd",
		"snippet": "\ue612",
		"operator": "\uf915",
		"reference": "\uf87a",
		"typeParameter": "\uf278",
		"folder": "\ue5ff",
		"default": "\uf29c"
	},
	"yank.priority": 0
}
