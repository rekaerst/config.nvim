		"interface": "\uf805",
		"keyword": "\uf767",
		"text": "\uf719",
		"enum": "\uf636",
		"enumMember": "\uf02b",
		"color": "\ue22b",
		"property": "\uf48d",
		"field": "\uf01c",
		"unit": "\uf475",
		"file": "\uf471",
		"value": "\uf8a3",
		"event": "\ufacd",
